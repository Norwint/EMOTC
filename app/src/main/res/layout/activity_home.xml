<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".views.activity.HomeActivity">

    <com.otcengineering.em.views.components.TitleBar
        android:id="@+id/home_titleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <FrameLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="54dp"
        android:layout_marginTop="54dp" >

    </FrameLayout>

    <include android:id="@+id/navBar"
        layout="@layout/view_navigation_tab" />

    <FrameLayout
        android:id="@+id/home_contentMenu"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

    </FrameLayout>

    <include
        android:id="@+id/menu_home"
        layout="@layout/menu_home" />
</FrameLayout>
